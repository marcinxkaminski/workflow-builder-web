{
  "homepage": "https://xkamson.github.io/ml-web/",
  "name": "ml-web",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "snyk test && CI=true react-scripts test",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint './src/{app,tests}/**/*.{js,jsx}'",
    "lint:fix": "eslint --fix './src/{app,tests}/**/*.{js,jsx}'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "snyk": "snyk test",
    "snyk:fix": "snyk protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.15.1",
    "gh-pages": "^2.1.1",
    "snyk": "^1.230.4"
  }
}
