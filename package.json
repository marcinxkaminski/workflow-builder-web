{
  "homepage": "https://marcinxkaminski.github.io/workflow-builder-web/",
  "name": "workflow-builder-web",
  "version": "1.0.0",
  "private": false,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-scripts": "3.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "rebuild": "npm run clean && npm install && npm run build",
    "test": "snyk test && CI=true react-scripts test",
    "test:coverage": "npm test -- --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint './src/**/*.{js,jsx}'",
    "lint:fix": "./node_modules/.bin/eslint --fix './src/**/*.{js,jsx}'",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "snyk": "snyk test",
    "snyk:fix": "snyk protect"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "codecov": "^3.6.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "node-sass": "^4.13.0",
    "redux-devtools": "^3.5.0",
    "redux-mock-store": "^1.5.3",
    "sass": "^1.23.7",
    "snyk": "^1.251.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "!src/index.jsx",
      "!src/serviceWorker.js",
      "!src/store.js",
      "src/**/*.{js,jsx}"
    ]
  }
}
